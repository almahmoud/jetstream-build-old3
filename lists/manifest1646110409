          - -c
        args:
          - cp -r /usr/local/lib/R/site-library/blockmodeling /mnt/libs/R/blockmodeling && cd /home/rstudio && ls | grep "blockmodeling" | xargs -i cp ./{} /mnt/libs/R/{}
