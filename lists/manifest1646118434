          - /bin/sh
          - -c
        args:
          - cp /mnt/libs/R/.rclone.conf ~/.rclone.conf && rclone copyto /usr/local/lib/R/site-library/linkcomm iujs:js-builder/libraries/RELEASE_3_14/container/linkcomm -v && cd /home/rstudio && ls | grep "linkcomm" | xargs -i rclone copyto ./{} iujs:js-builder/packages/RELEASE_3_14/container/{} -v
