          - -c
        args:
          - cp -r /usr/local/lib/R/site-library/KEGGgraph /mnt/libs/R/KEGGgraph && cd /home/rstudio && ls | grep "KEGGgraph" | xargs -i cp ./{} /mnt/libs/R/{}
