          - -c
        args:
          - cp -r /usr/local/lib/R/site-library/R.filesets /mnt/libs/R/R.filesets && cd /home/rstudio && ls | grep "R.filesets" | xargs -i cp ./{} /mnt/libs/R/{}
        volumeMounts:
          - name: libraries
            mountPath: "/mnt/libs/R"
          - name: tmplibrary
            mountPath: "/usr/local/lib/R/site-library"
