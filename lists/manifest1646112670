          - -c
        args:
          - cp /mnt/libs/R/.rclone.conf ~/.rclone.conf && rclone copyto /usr/local/lib/R/site-library/conicfit osn:bir190004-bucket01/libraries/RELEASE_3_14/container/conicfit -v && cd /home/rstudio && ls | grep "conicfit" | xargs -i rclone copyto ./{} osn:bir190004-bucket01/packages/RELEASE_3_14/container/{} -v
        volumeMounts:
