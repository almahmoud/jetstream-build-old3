          - /bin/bash
          - -c
        args:
          - cp -r /usr/local/lib/R/site-library/locfit /mnt/libs/R/locfit && cd /home/rstudio && ls | grep "locfit" | xargs -i cp ./{} /mnt/libs/R/{}
