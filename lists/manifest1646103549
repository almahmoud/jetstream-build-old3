          - cp /mnt/libs/R/.rclone.conf ~/.rclone.conf && rclone copyto /usr/local/lib/R/site-library/robustbase iujs:js-builder/libraries/RELEASE_3_14/container/robustbase -v && cd /home/rstudio && ls | grep "robustbase" | xargs -i rclone copyto ./{} iujs:js-builder/packages/RELEASE_3_14/container/{} -v
        volumeMounts:
