        command:
          - /bin/bash
          - -c
        args:
          - cp -r /usr/local/lib/R/site-library/transport /mnt/libs/R/transport && cd /home/rstudio && ls | grep "transport" | xargs -i cp ./{} /mnt/libs/R/{}
